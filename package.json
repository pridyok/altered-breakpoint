{
  "name": "altered-js",
  "version": "1.0.0",
  "description": "Javascript development environment",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "sass": "src/index.sass",
  "files": [
    "/dist"
  ],
  "jest": {
    "verbose": true,
    "roots": [
      "test/"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    }
  },
  "repository": "github:pridyok/altered-js",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel-node build/server.js",
    "lint": "eslint .",
    "lint:watch": "npm run lint -- --watch",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "pretest": "tsc test/*.ts --noEmit",
    "test": "jest",
    "prebuild": "run-p clean-dist lint",
    "build": "cross-env NODE_ENV=production babel-node build/build.js",
    "postbuild": "cross-env NODE_ENV=production babel-node build/server.js"
  },
  "keywords": [],
  "author": "Aaron Pridy",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.10.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.14",
    "@types/eslint": "^7.2.10",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.0.2",
    "babel-register": "^6.26.0",
    "bufferutil": "^4.0.3",
    "canvas": "^2.7.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.1",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "fibers": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^6.2.1",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "js-yaml": "^4.1.0",
    "mini-css-extract-plugin": "^1.4.1",
    "npm-run-all": "^4.1.5",
    "parallel-webpack": "^2.6.0",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "source-map-loader": "^2.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "utf-8-validate": "^5.0.4",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-merge": "^5.7.3"
  }
}
